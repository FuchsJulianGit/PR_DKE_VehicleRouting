-- Create the PERSON table
CREATE TABLE PERSON (
    ID NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY,
    NAME VARCHAR(255),
    START_COORDINATE VARCHAR(255),
    END_COORDINATE VARCHAR(255),
    COMPANY VARCHAR(255),
    NEEDS_WHEELCHAIR Number(1),
    CONSTRAINT PK_PERSON PRIMARY KEY(ID)
);

