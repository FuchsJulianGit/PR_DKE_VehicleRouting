DROP TABLE ROUTE_POINTS;

CREATE TABLE ROUTE_POINTS (
    ID NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY,
    SEQ NUMBER,
    START_POINT NUMBER,
    PERSON NUMBER,
    CONSTRAINT PK_ROUTE_POINTS PRIMARY KEY (ID)
);


INSERT INTO ROUTE_POINTS (SEQ, START_POINT, PERSON)
VALUES (1, 10, 1);

INSERT INTO ROUTE_POINTS (SEQ, START_POINT, PERSON)
VALUES (2, 20, 2);

INSERT INTO ROUTE_POINTS (SEQ, START_POINT, PERSON)
VALUES (3, 30, 3);

INSERT INTO ROUTE_POINTS (SEQ, START_POINT, PERSON)
VALUES (4, 40, 4);
