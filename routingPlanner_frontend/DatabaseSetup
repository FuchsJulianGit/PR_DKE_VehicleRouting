CREATE TABLE ROUTE_POINTS (
    ID NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY,
    DESCRIPTION VARCHAR2(255), -- Adjust the data type and length as needed
    SEQUENZ NUMBER,
    ATHOME NUMBER,
    COORDINATES VARCHAR2(255), -- Adjust the data type and length as needed
    VEHICLE NUMBER,
    COORDINATE_ID NUMBER,
    CONSTRAINT PK_ROUTE_POINTS PRIMARY KEY (ID)
);

INSERT INTO ROUTE_POINTS (DESCRIPTION, SEQUENZ, ATHOME, COORDINATES, VEHICLE, COORDINATE_ID)
VALUES ('Description 1', 1, 1, '[10.1234,20.5678]', 1, 1);

INSERT INTO ROUTE_POINTS (DESCRIPTION, SEQUENZ, ATHOME, COORDINATES, VEHICLE, COORDINATE_ID)
VALUES ('Description 2', 2, 0, '[30.9876,40.5432]', 2, 1);

INSERT INTO ROUTE_POINTS (DESCRIPTION, SEQUENZ, ATHOME, COORDINATES, VEHICLE, COORDINATE_ID)
VALUES ('Description 3', 3, 1, '[50.1357,60.2468]', 3, 1);

INSERT INTO ROUTE_POINTS (DESCRIPTION, SEQUENZ, ATHOME, COORDINATES, VEHICLE, COORDINATE_ID)
VALUES ('Description 4', 4, 0, '[70.8642,80.9753]', 4, 1);

DROP TABLE VEHICLES;

CREATE TABLE VEHICLES (
    ID NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY,
    COMPANY_NAME VARCHAR2(255),
    VEHICLE_DESCRIPTION VARCHAR2(255),
    START_COORDINATE VARCHAR2(255),
    END_COORDINATE VARCHAR2(255),
    CAN_TRANSPORT_WHEELCHAIRS NUMBER(1),
    SEATING_PLACES NUMBER,
    CONSTRAINT PK_VEHICLES PRIMARY KEY (ID)
);
